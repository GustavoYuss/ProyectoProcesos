<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="395.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mx.uv.fei.GUI.EditarProductoController">
    <stylesheets>
        <URL value="@editarproducto.css" />
    </stylesheets>
   <children>
      <Label layoutX="77.0" layoutY="49.0" prefHeight="22.0" prefWidth="261.0" text="Editar Producto del Inventario">
         <font>
            <Font name="Rockwell" size="18.0" />
         </font>
      </Label>
      <Label layoutX="48.0" layoutY="89.0" prefHeight="22.0" prefWidth="182.0" text="Nombre del Producto">
         <font>
            <Font name="Rockwell" size="12.0" />
         </font>
      </Label>
      <Label layoutX="48.0" layoutY="144.0" prefHeight="22.0" prefWidth="182.0" text="Precio del Producto">
         <font>
            <Font name="Rockwell" size="12.0" />
         </font>
      </Label>
      <Label layoutX="48.0" layoutY="199.0" prefHeight="22.0" prefWidth="182.0" text="Dosis del Producto">
         <font>
            <Font name="Rockwell" size="12.0" />
         </font>
      </Label>
      <TextField fx:id="txtNombreProducto" layoutX="48.0" layoutY="111.0" prefHeight="26.0" prefWidth="317.0" promptText="Escriba aquí el nombre del producto" />
      <TextField fx:id="txtPrecioProducto" layoutX="49.0" layoutY="166.0" prefHeight="26.0" prefWidth="317.0" promptText="Escriba aquí el precio del producto" />
      <TextField fx:id="txtDosisProducto" layoutX="48.0" layoutY="221.0" prefHeight="26.0" prefWidth="317.0" promptText="Escriba aquí la dosis del producto" />
      <Button fx:id="btnGuardarcambio" layoutX="81.0" layoutY="286.0" mnemonicParsing="false" onAction="#guardarCambios" text="Guardar cambios">
         <font>
            <Font name="Rockwell" size="12.0" />
         </font>
      </Button>
      <Button fx:id="btnCancelar" layoutX="237.0" layoutY="286.0" mnemonicParsing="false" onAction="#salir" text="Cancelar">
         <font>
            <Font name="Rockwell" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
