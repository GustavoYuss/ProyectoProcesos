<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="395.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mx.uv.fei.GUI.AgregarProductoAlInventarioController">
    <stylesheets>
        <URL value="@agregarproductoalinventario.css" />
    </stylesheets>
   <children>
      <AnchorPane layoutX="38.0" layoutY="27.0" prefWidth="0.0" />
      <Label layoutX="67.0" layoutY="39.0" prefHeight="22.0" prefWidth="261.0" text="Agregar Producto al Inventario">
         <font>
            <Font name="Rockwell" size="18.0" />
         </font>
      </Label>
      <Label layoutX="38.0" layoutY="109.0" prefHeight="22.0" prefWidth="182.0" text="Nombre del Producto">
         <font>
            <Font name="Rockwell" size="12.0" />
         </font>
      </Label>
      <Label layoutX="38.0" layoutY="164.0" prefHeight="22.0" prefWidth="182.0" text="Precio del Producto">
         <font>
            <Font name="Rockwell" size="12.0" />
         </font>
      </Label>
      <Label layoutX="38.0" layoutY="219.0" prefHeight="22.0" prefWidth="182.0" text="Dosis del Producto">
         <font>
            <Font name="Rockwell" size="12.0" />
         </font>
      </Label>
      <TextField fx:id="txtNombreProducto" layoutX="38.0" layoutY="131.0" prefHeight="26.0" prefWidth="317.0" promptText="Escriba aquí el nombre del producto" />
      <TextField fx:id="txtPrecioProducto" layoutX="39.0" layoutY="186.0" prefHeight="26.0" prefWidth="317.0" promptText="Escriba aquí el precio del producto" />
      <TextField fx:id="txtDosisProducto" layoutX="38.0" layoutY="241.0" prefHeight="26.0" prefWidth="317.0" promptText="Escriba aquí la dosis del producto" />
      <Button fx:id="btnGuardarProducto" layoutX="71.0" layoutY="346.0" mnemonicParsing="false" onAction="#guardarProducto" text="Guardar producto">
         <font>
            <Font name="Rockwell" size="12.0" />
         </font>
      </Button>
      <Button fx:id="btnCancelar" layoutX="231.0" layoutY="346.0" mnemonicParsing="false" onAction="#salir" text="Cancelar">
         <font>
            <Font name="Rockwell" size="12.0" />
         </font>
      </Button>
      <ComboBox fx:id="cmbLugares" layoutX="38.0" layoutY="70.0" prefHeight="26.0" prefWidth="318.0" promptText="Sucursal" />
      <Label layoutX="39.0" layoutY="276.0" prefHeight="22.0" prefWidth="182.0" text="Cantidad del producto">
         <font>
            <Font name="Rockwell" size="12.0" />
         </font>
      </Label>
      <TextField fx:id="txtcantidad" layoutX="39.0" layoutY="298.0" prefHeight="26.0" prefWidth="317.0" promptText="Escriba aquí la cantidad del producto" />
   </children>
</AnchorPane>
